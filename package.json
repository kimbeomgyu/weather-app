{
  "name": "weather-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/react-redux": "^7.0.8",
    "axios": "^0.18.0",
    "moment": "^2.24.0",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-scripts-ts": "3.1.0",
    "redux": "^4.0.1",
    "redux-promise": "^0.6.0"
  },
  "scripts": {
    "build:css": "node-sass src/assets/scss/ -o src/assets/css/",
    "watch:css": "npm run build:css && node-sass src/assets/scss/ -o src/assets/css/ --watch --recursive",
    "start:js": "react-scripts-ts start",
    "start": "npm-run-all -p watch:css start:js",
    "build": "npm run build:css && react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/jest": "^24.0.12",
    "@types/node": "^12.0.0",
    "@types/node-sass": "^4.11.0",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "npm-run-all": "^4.1.5",
    "typescript": "^3.4.5"
  }
}
